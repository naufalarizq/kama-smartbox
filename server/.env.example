########################################
# Example environment for KAMA Smartbox #
########################################

# 1) Sectioned INI style (recommended)
[realtime_db]
host = localhost
port = 5432
user = postgres
password = satudua3
dbname = kama_realtime
# sslmode = require

[server_db]
# Optional: if omitted, will fall back to [realtime_db]
# host = localhost
# port = 5432
# user = postgres
# password = satudua3
# dbname = kama_server
# sslmode = require

# 2) Or flat env vars (also supported)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=kama_realtime
# DB_USER=postgres
# DB_PASS=satudua3
# SERVER_DB_HOST=localhost
# SERVER_DB_PORT=5432
# SERVER_DB_NAME=kama_server
# SERVER_DB_USER=postgres
# SERVER_DB_PASS=satudua3
# SERVER_DB_SSLMODE=require

# 3) Gemini API Key
# GEMINI_API_KEY="GANTI_DENGAN_API_KEY_ANDA"
